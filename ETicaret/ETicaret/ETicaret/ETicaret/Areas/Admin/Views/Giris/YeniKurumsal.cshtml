@model ETicaret.Models.Kurumsal
@{
    ViewBag.Title = "YeniKurumsal";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Yeni Urun Ekleme</h2>
<br />
<form class="form-group" method="post">
  
    @Html.LabelFor(x => x.ImageUrl)
    @Html.TextAreaFor(x => x.ImageUrl,new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Aciklama)
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Ekle</button>
</form>

